<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.waybillInfo.mapper.WaybillNoticeHistoryMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  waybill_notice_history 
		WHERE
			 waybill_no = #{mainId} 	</delete>

	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.waybillInfo.entity.WaybillNoticeHistory">
		SELECT *
		FROM  waybill_notice_history
		WHERE
			 waybill_no = #{mainId} 	</select>
</mapper>
